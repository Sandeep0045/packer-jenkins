{
  "variables": {
    "aws_access_key": "{{env 'AWS_KEY' }}"
    "aws_secret_key": "{{env 'AWS_SECRET' }}"
    "ssh_private_key": "{{env 'SSH_PRIVATE_KEY' }}"
    
  },
  "builders": [
    {
      "type": "amazon-ebs",
      "region": "us-east-2",
      "source_ami": "ami-0f57b4cec24530068",
