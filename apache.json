      "ssh_priavte_key_file" : "{{user 'ssh_private_key'}}"
      "ami_name": "apache-ami-packer-{{timestamp}}"
    }
  ],
  "provisioners": [
    {
      "type": "ansible",
      "playbook_file": "./playbook.yml"
    }
  ]
}
